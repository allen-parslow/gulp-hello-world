{
  "env":{
    "browser":true,
    "es6":true
  },
  "extends": ["airbnb", "plugin:lodash/recommended"],
  "globals":{
    "Atomics":"readonly",
    "SharedArrayBuffer":"readonly"
  },
  "parserOptions":{
    "ecmaFeatures":{
      "jsx":true
    },
    "ecmaVersion":2018,
    "sourceType":"module"
  },
  "plugins": ["lodash"],
  "rules":{
    "indent":[
      "error",
      4
    ],
    "no-console":"off",
    "quotes":[
      "error",
      "single"
    ],
    "comma-dangle":[
      "error",
      "never"
    ],
    "arrow-body-style":[
      "error",
      "always"
    ]
  }
}